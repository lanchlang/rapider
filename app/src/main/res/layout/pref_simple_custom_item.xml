<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    tools:background="@color/photonWhite"
    android:layout_height="60dp">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/end_icon"
        android:layout_width="22dp"
        android:layout_height="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/ic_right_20"
        android:layout_marginEnd="16dp"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@android:id/summary"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/end_icon"
        android:layout_marginEnd="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="@string/google"
        style="@style/subTitle"
        />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@android:id/title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@android:id/summary"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="10dp"
        tools:text="@string/normal_tab_page"
        style="@style/title"
        />
</androidx.constraintlayout.widget.ConstraintLayout>